{
    "title": "PhotoStats",
    "authors": [ "Mike Morano <mmorano@mikeandwan.us>" ],
    "description": "Tool to try and find optiimzation parameters for the SizePhotos tool",
    "version": "0.2.0",
    "packOptions": {
        "projectUrl": "https://github.com/AerisG222/PhotoStats",
        "licenseUrl": "https://github.com/AerisG222/PhotoStats/blob/master/LICENSE.md",
